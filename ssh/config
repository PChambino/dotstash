Host *
   AddKeysToAgent yes
   UseKeychain yes
Host localhost
   StrictHostKeyChecking no
   UserKnownHostsFile /dev/null
Host 127.*
   StrictHostKeyChecking no
   UserKnownHostsFile /dev/null
Host 0.*
   StrictHostKeyChecking no
   UserKnownHostsFile /dev/null
Host 10.*
   StrictHostKeyChecking no
   UserKnownHostsFile /dev/null
Host 192.168.*
   StrictHostKeyChecking no
   UserKnownHostsFile /dev/null
Host 172.31.*
   StrictHostKeyChecking no
   UserKnownHostsFile /dev/null
Host box
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ~/box/.vagrant/machines/default/virtualbox/private_key
  IdentitiesOnly yes
  LogLevel QUIET
  ForwardAgent yes
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%C
  ControlPersist 3600
